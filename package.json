  "scripts": {
    "start": "NODE_ENV=development bash -c './node_modules/webpack-dev-server/bin/webpack-dev-server.js -d --hot --inline --config webpack.config.babel.js'",
    "webpack": "./node_modules/webpack/bin/webpack.js --config webpack.config.babel.js",
    "build": "webpack",
    "dist": "NODE_ENV=production bash -c 'npm run clean && npm run webpack'",
    "clean": "rm -rf ./dist/*",
    "test": "export NODE_ENV=testing|| set NODE_ENV=testing && NODE_PATH=./src mocha src/test/setup.js \"src/**/test.*.es\" --require babel-register --require test/null-compiler",
